{#
/**
 * @file
 * Theme override for the basic structure of a single Drupal page.
 *
 * Variables:
 * - logged_in: A flag indicating if user is logged in.
 * - root_path: The root path of the current page (e.g., node, admin, user).
 * - node_type: The content type for the current node, if the page is a node.
 * - head_title: List of text elements that make up the head_title variable.
 *   May contain or more of the following:
 *   - title: The title of the page.
 *   - name: The name of the site.
 *   - slogan: The slogan of the site.
 * - page_top: Initial rendered markup. This should be printed before 'page'.
 * - page: The rendered page markup.
 * - path_info.args: Array of URL arguments un aliassed.
 * - page_bottom: Closing rendered markup. This variable should be printed after
 *   'page'.
 * - db_offline: A flag indicating if the database is offline.
 * - placeholder_token: The token for generating head, css, js and js-bottom
 *   placeholders.
 *
 * @see template_preprocess_html()
 */
#}
{%
  set body_classes = [
    logged_in ? 'user-logged-in',
    not root_path ? 'path-frontpage' : 'path-' ~ root_path|clean_class,
    node_type ? 'node--type-' ~ node_type|clean_class,
    db_offline ? 'db-offline',
  ]
%}
<!DOCTYPE html>
<html{{ html_attributes }}>
  <head>
    <head-placeholder token="{{ placeholder_token|raw }}">
    <title>{{ head_title|safe_join(' | ') }}</title>
    <css-placeholder token="{{ placeholder_token|raw }}">
    <js-placeholder token="{{ placeholder_token|raw }}">


      <link rel="apple-touch-icon" sizes="57x57"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-57x57.png">
      <link rel="apple-touch-icon" sizes="60x60"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-60x60.png">
      <link rel="apple-touch-icon" sizes="72x72"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-72x72.png">
      <link rel="apple-touch-icon" sizes="76x76"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-76x76.png">
      <link rel="apple-touch-icon" sizes="114x114"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-114x114.png">
      <link rel="apple-touch-icon" sizes="120x120"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-120x120.png">
      <link rel="apple-touch-icon" sizes="144x144"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-144x144.png">
      <link rel="apple-touch-icon" sizes="152x152"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-152x152.png">
      <link rel="apple-touch-icon" sizes="180x180"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-180x180.png">
      <link rel="icon" type="image/png" sizes="192x192"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-192x192.png">
      <link rel="icon" type="image/png" sizes="32x32"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-32x32.png">
      <link rel="icon" type="image/png" sizes="96x96"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-96x96.png">
      <link rel="icon" type="image/png" sizes="16x16"
            href="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-16x16.png">
      <meta name="msapplication-TileColor" content="#ffffff">
      <meta name="msapplication-TileImage"
            content="/themes/custom/expo2020/assets/src/images/favicon/expo-favicon-144x144.png">
      <meta name="theme-color" content="#ffffff">
  </head>
      <!-- Global site tag (gtag.js) - Google Analytics -->
      <script async src="https://www.googletagmanager.com/gtag/js?id=UA-28534375-2"></script>
      <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-28534375-2');
      </script>
  <body{{ attributes.addClass(body_classes) }}>
    <a href="#main-content" class="visually-hidden focusable skip-link">
      {{ 'Skip to main content'|t }}
    </a>
    <a href="#block-mainnavigation" class="visually-hidden focusable skip-link">
      {{ 'Skip to main nagivation'|t }}
    </a>
    {{ page_top }}
    {{ page }}
    {{ page_bottom }}
    <js-bottom-placeholder token="{{ placeholder_token|raw }}">
  </body>
</html>
